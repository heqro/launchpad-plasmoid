<?xml version="1.0" encoding="UTF-8"?>
<kcfg xmlns="http://www.kde.org/standards/kcfg/1.0"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.kde.org/standards/kcfg/1.0
      http://www.kde.org/standards/kcfg/1.0/kcfg.xsd" >
  <kcfgfile name=""/>

  <group name="General">
    <entry name="icon" type="String">
      <default>start-here-kde</default>
    </entry>
    <entry name="useCustomButtonImage" type="Bool">
      <default>false</default>
    </entry>
    <entry name="tooltipsInGrid" type="Bool">
        <default>false</default>
    </entry>
    <entry name="favoritesInGrid" type="Bool">
        <default>false</default>
    </entry>
    <entry name="writeSomething" type="Bool">
        <default>false</default>
    </entry>
    <entry name="greetingText" type="String">
        <default></default>
    </entry>
    <entry name="opacitySet" type="Bool">
        <default>false</default>
    </entry>
    <entry name="alphaValue" type="Double">
        <default>0.6</default>
    </entry>
    <entry name="showLabelBackground" type="Bool">
      <default>false</default>
    </entry>
    <entry name="labelTransparency" type="Double">
      <default>0.6</default>
    </entry>
    <entry name="showCategoriesBackground" type="Bool">
      <default>false</default>
    </entry>
    <entry name="categoriesTransparency" type="Double">
      <default>0.6</default>
    </entry>
    <entry name="showItemGridBackground" type="Bool">
      <default>false</default>
    </entry>
    <entry name="itemGridTransparency" type="Double">
      <default>0.6</default>
    </entry>
    <entry name="showSessionControlBackground" type="Bool">
      <default>false</default>
    </entry>
    <entry name="sessionControlTransparency" type="Double">
      <default>0.6</default>
    </entry>
    <entry name="isBackgroundImageSet" type="Bool">
      <default>false</default>
    </entry>
    <entry name="backgroundImage" type="String">
      <label>The path to the file indicating the background image.</label>
      <default></default>
    </entry>
    <entry name="blurSamples" type="Int">
      <label>Samples input for the gaussian blur.</label>
      <default>9</default>
    </entry>
    <entry name="blurRadius" type="Int">
      <label>Radius input for the gaussian blur.</label>
      <default>4</default>
    </entry>
    <entry name="isBlurEnabled" type="Bool">
      <label></label>
      <default>false</default>
    </entry>
    <entry name="isWallpaperBlurred" type="Bool">
      <label>When blur is enabled, determine whether or not to blur the user's wallpaper image.</label>
      <default>false</default>
    </entry>
    <entry name="categoriesText" type="Bool">
        <default>false</default>
    </entry>
    <entry name="categoriesIcon" type="Bool">
        <default>false</default>
    </entry>
    <entry name="categoriesIconAndText" type="Bool">
        <default>true</default>
    </entry>
    <entry name="customizeCategoriesFontSize" type="Bool">
        <default>false</default>
    </entry>
    <entry name="categoriesFontSize" type="Int">
        <default>15</default>
    </entry>
    <entry name="searchBarDesign" type="String">
        <default>searchbar_designs/FullyFledged.qml</default>
    </entry>
    <entry name="clickToToggle" type="Bool">
      <default>true</default>
    </entry>
    <entry name="configureOpacity" type="Bool">
        <default>false</default>
    </entry>
    <entry name="searchBarOpacity" type="Double">
        <default>0</default>
    </entry>
    <entry name="numberColumns" type="Int">
      <default>7</default>
    </entry>
    <entry name="numberRows" type="Int">
      <default>5</default>
    </entry>
    <entry name="applicationButtonHeight" type="Int">
      <default>132</default>
    </entry>
    <entry name="applicationButtonWidth" type="Int">
      <default>132</default>
    </entry>
    <entry name="labelLines" type="Int">
      <default>2</default>
    </entry>
    <entry name="favoritesIconSize" type="Int">
      <default>108</default>
    </entry>
    <entry name="startOnFavorites" type="Bool">
        <default>false</default>
    </entry>
    <entry name="showCategories" type="Bool">
        <default>true</default>
    </entry>
    <entry name="hideCategories" type="Bool">
        <default>false</default>
    </entry>
    <entry name="showFavoritesCategory" type="Bool">
        <default>true</default>
    </entry>
    <entry name="showRecentFilesCategory" type="Bool">
        <default>true</default>
    </entry>
    <entry name="showRecentAppsCategory" type="Bool">
        <default>true</default>
    </entry>
    <entry name="showCategoriesTooltip" type="Bool">
        <default>false</default>
    </entry>
    <entry name="showCategoriesOnTheRight" type="Bool">
        <default>true</default>
    </entry>
    <entry name="customizeCategoriesButtonSize" type="Bool">
        <default>false</default>
    </entry>
    <entry name="categoriesButtonHeight" type="Int">
        <default>50</default>
    </entry>
    <entry name="categoriesButtonWidth" type="Int">
        <default>200</default>
    </entry>
    <entry name="showSessionControlBar" type="Bool">
        <default>true</default>
    </entry>
    <entry name="showSessionControlTooltips" type="Bool">
        <default>true</default>
    </entry>
    <entry name="paginateGrid" type="Bool">
      <default>false</default>
    </entry>
    <entry name="hiddenApplicationsName" type="StringList">
        <default></default>
    </entry>
    <entry name="hiddenApplicationsDescription" type="StringList">
        <default></default>
    </entry>
    <entry name="hiddenApplicationsUrl" type="StringList">
        <default></default>
    </entry>
    <entry name="hiddenApplicationsIcon" type="StringList">
        <default></default>
    </entry>

    <entry name="customButtonImage" type="Url">
      <default></default>
    </entry>

    <entry name="showFilterList" type="Bool">
      <default>true</default>
    </entry>

    <entry name="appNameFormat" type="Int">
      <default>0</default>
    </entry>
    <entry name="limitDepth" type="Bool">
      <default>false</default>
    </entry>
    <entry name="favoriteApps" type="StringList">
      <default></default>
    </entry>
    <entry name="favoriteSystemActions" type="StringList">
      <default>logout,lock-screen,reboot,shutdown</default>
    </entry>
    <entry name="hiddenApplications" type="StringList">
      <default></default>
    </entry>
    <entry name="useExtraRunners" type="Bool">
      <default>true</default>
    </entry>
    <entry name="extraRunners" type="StringList">
      <!--<default>bookmarks,baloosearch,calculator</default>-->
      <default>calculator</default>
    </entry>
    <entry name="alignResultsToBottom" type="Bool">
      <default>true</default>
    </entry>
    <entry name="switchCategoriesOnHover" type="Bool">
      <label>Whether to switch between menu categories by hovering them.</label>
      <default>false</default>
    </entry>
    <entry name="favoritesPortedToKAstats" type="Bool">
      <label>Are the favorites ported to use KActivitiesStats to allow per-activity favorites</label>
      <default>false</default>
    </entry>
  </group>
</kcfg>
